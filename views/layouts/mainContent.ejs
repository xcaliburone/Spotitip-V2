<div class="content p-5 flex flex-col gap-y-6 flex-wrap text-wrap">

    <div class="header flex justify-between">
        <%- include('./main-buttonleft-user') %>
        <%- include('./main-buttonleft-artist') %>

        <div class="box relative">
            <form name="search">
                <input type="text" id="searchInput" class="input font-semibold" name="key" 
                placeholder="Search..." autocomplete="off">
            </form>
        </div>
    </div>

    <div class="main flex gap-y-9 <%= onlyUserContent %>">

        <%- include('./contentMain/newest-song') %>    
        
        <%- include('./contentMain/artist-popular') %>    
        
        <%- include('./contentMain/album-popular') %>
        
        <%- include('./contentMain/playlist-popular') %>
        
        <%- include('./contentMain/newest-user') %>

    </div>
    <div class="main flex gap-y-9 <%= onlyArtistContent %>">

        <%- include('./contentMain/artist-albums') %>
        
        <%- include('./contentMain/artist-songs') %>

    </div>
</div>